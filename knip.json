{
    "$schema": "https://unpkg.com/knip@5/schema.json",
    "entry": [
        "src/app/**/*.{js,jsx,ts,tsx}",
        "src/pages/**/*.{js,jsx,ts,tsx}",
        "src/middleware.ts",
        "scripts/*.{js,ts}",
        "tailwind.config.ts",
        "next.config.mjs"
    ],
    "project": [
        "src/**/*.{js,jsx,ts,tsx}",
        "scripts/**/*.{js,ts}"
    ],
    "ignore": [
        "src/components/ui/**",
        "src/dataconnect-generated/**",
        "**/*.d.ts",
        ".agent/**",
        ".cache/**",
        "artifacts/**"
    ],
    "ignoreExportsUsedInFile": true,
    "ignoreDependencies": [
        "postcss",
        "autoprefixer",
        "sharp"
    ]
}