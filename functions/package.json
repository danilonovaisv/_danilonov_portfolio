{
  "name": "functions",
  "main": "lib/index.js",
  "scripts": {
    "genkit:start": "genkit start -- tsx --env-file=.env --watch src/genkit/sample.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "build": "tsc",
    "build:watch": "tsc --watch",
    "typecheck": "tsc --noEmit",
    "analyze": "depcheck && madge --circular src/",
    "clean": "rimraf lib",
    "serve": "npm run build && firebase emulators:start --only apphosting",
    "shell": "npm run build && firebase functions:shell",
    "start": "npm run shell",
    "deploy": "firebase deploy --only hosting --project portfolio-danilo-novais",
    "logs": "firebase functions:log"
  },
  "engines": {
    "node": ">=22"
  },
  "dependencies": {},
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "depcheck": "^1.4.7",
    "eslint": "^9.17.0",
    "globals": "^15.13.0",
    "madge": "^6.1.0",
    "genkit-cli": "^1.26.0",
    "tsx": "^4.19.0",
    "typescript": "^5.7.0",
    "typescript-eslint": "^8.18.0"
  },
  "private": true
}
