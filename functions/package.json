    "firebase-functions": "^6.0.0",
    "genkit": "^1.26.0"
  },
  "devDependencies": {
    "@eslint/js": "^9.17.0",
    "@types/express": "^4.17.21",
    "depcheck": "^1.4.7",
    "eslint": "^9.17.0",
    "globals": "^15.13.0",
    "madge": "^6.1.0",
    "firebase-functions-test": "^3.4.0",
    "genkit-cli": "^1.26.0",
