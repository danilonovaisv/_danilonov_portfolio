{
  "name": "danilo-novais-portfolio-backup",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "react",
    "three",
    "threejs",
    "react-three-fiber"
  ],
  "engines": {
    "node": "22"
  },
  "scripts": {
    "dev": "next dev",
    "prebuild": "node scripts/generate-build-info.cjs",
    "build": "next build",
    "start": "next start",
    "lint": "eslint --ext .js,.ts,.tsx .",
    "lint:fix": "eslint --ext .js,.ts,.tsx . --fix",
    "eslint": "eslint --fix .",
    "eslint:ci": "eslint .",
    "prettier": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "typecheck": "tsc --noEmit --emitDeclarationOnly false --strict --jsx react-jsx",
    "typegen": "tsc --emitDeclarationOnly",
    "analyze": "npm run lint && npm run typecheck",
    "analyze:deps": "depcheck",
    "analyze:graph": "node scripts/analyze-graph.cjs",
    "test": "npm run eslint:ci && npm run typecheck && npm run prettier && npm run jest && (cd test/e2e; ./e2e.sh 0.180)",
    "jest": "jest",
    "type-check": "tsc --noEmit",
    "format": "prettier --write .",
    "clean": "rimraf .next"
  },
  "dependencies": {
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.4.2",
    "@react-three/postprocessing": "^3.0.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.88.0",
    "clsx": "^2.1.1",
    "framer-motion": "^12.23.26",
    "lenis": "^1.3.16",
    "lucide-react": "^0.561.0",
    "next": "^16.0.10",
    "react": "^19.2.3",
    "react-dom": "^19.2.3",
    "resize-observer-polyfill": "^1.5.1",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "zod": "^4.2.1"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@types/jest": "^30.0.0",
    "@types/node": "^25.0.3",
    "@types/react": "^19.2.7",
    "@types/react-dom": "^19.2.3",
    "@types/three": "^0.182.0",
    "@typescript-eslint/eslint-plugin": "^8.50.0",
    "@typescript-eslint/parser": "^8.50.0",
    "autoprefixer": "^10.4.23",
    "depcheck": "^1.4.7",
    "eslint": "^9.39.2",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.7.4",
    "rimraf": "^6.1.2",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.9.3"
  },
  "overrides": {
    "test-exclude": "7.0.1"
  }
}
