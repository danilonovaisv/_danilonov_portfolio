{
  "name": "danilo-novais-portfolio",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "keywords": [
    "react",
    "three",
    "threejs",
    "react-three-fiber"
  ],
  "engines": {
    "node": "22"
  },
  "packageManager": "npm@11.7.0",
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "build:prod": "npm run prebuild && next build",
    "start": "next start",
    "clean": "rimraf .next out .eslintcache .firebase",
    "prebuild": "node scripts/generate-build-info.cjs",
    "lint": "eslint --cache --cache-location .eslintcache scripts src test tailwind.config.ts",
    "lint:fix": "eslint --cache --cache-location .eslintcache scripts src test tailwind.config.ts --fix && prettier --write .",
    "typecheck": "tsc --noEmit --strict --jsx react-jsx",
    "test": "npm run lint && npm run typecheck && jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "format": "prettier --write .",
    "analyze": "npm run lint && npm run typecheck",
    "analyze:deps": "depcheck",
    "analyze:bundle": "ANALYZE=true next build",
    "deploy": "bash scripts/deploy-with-env.sh",
    "deploy:hosting": "npm run build && firebase deploy --only hosting --debug",
    "deploy:enable-web": "firebase experiments:enable webframeworks",
    "update": "npx npm-check-updates -u && npm install",
    "manage-keys": "tsx scripts/manage-service-account-keys.ts --account=firebase-adminsdk-fbsvc@portfolio-danilo-novais.iam.gserviceaccount.com",
    "assets:sync": "tsx scripts/sync-site-assets.ts assets.json",
    "assets:clean": "tsx scripts/repair-site-assets-clean.ts",
    "assets:repair": "tsx scripts/repair-site-assets-paths.ts",
    "assets:fix": "tsx scripts/repair-comprehensive.ts",
    "assets:sync:about": "tsx scripts/sync-site-assets.ts assets-about.json",
    "assets:check-links": "tsx scripts/check-storage-links.ts",
    "assets:check-unused": "node scripts/check-unused-assets.cjs",
    "assets:fix-duplicates": "tsx scripts/fix-duplicate-assets.ts",
    "assets:update-urls": "tsx scripts/update-supabase-urls.ts",
    "assets:verify-corrections": "tsx scripts/verify-asset-corrections.ts",
    "analyze:project": "tsx scripts/analyze-project.ts",
    "cleanup:project": "bash scripts/cleanup-project.sh",
    "prepare-deploy": "tsx scripts/prep-deploy.ts",
    "validate-env": "tsx scripts/validate-env.ts",
    "test:portfolio": "node scripts/test-portfolio-page.js",
    "postinstall": "npm run prebuild"
  },
  "dependencies": {
    "@dataconnect/admin-generated": "file:src/dataconnect-admin-generated",
    "@dataconnect/generated": "file:src/dataconnect-generated",
    "@genkit-ai/google-genai": "^1.27.0",
    "@google/adk": "^0.2.4",
    "@gsap/react": "^2.1.2",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/postprocessing": "^3.0.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.90.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^12.8.0",
    "firebase-admin": "^13.6.0",
    "firebase-frameworks": "^0.4.2",
    "firebase-functions": "^7.0.3",
    "frame": "1.0.0-alpha.2",
    "framer-motion": "^12.26.2",
    "gsap": "^3.14.2",
    "lenis": "^1.3.17",
    "lucide-react": "^0.562.0",
    "maath": "^0.10.8",
    "motion": "^12.26.2",
    "motion-studio-mcp": "https://api.motion.dev/registry.tgz?package=motion-studio-mcp&version=latest",
    "next": "^16.1.3",
    "postprocessing": "^6.38.2",
    "react": "^19.2.3",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.3",
    "react-hook-form": "^7.71.1",
    "recharts": "^3.6.0",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "three-stdlib": "^2.36.1",
    "zod": "^4.3.5",
    "zustand": "^5.0.10"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@next/bundle-analyzer": "^16.1.3",
    "@next/eslint-plugin-next": "^16.1.3",
    "@playwright/test": "^1.57.0",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.1",
    "@testing-library/user-event": "^14.6.1",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "25.0.9",
    "@types/react": "19.2.8",
    "@types/react-dom": "19.2.3",
    "@types/three": "^0.182.0",
    "@typescript-eslint/eslint-plugin": "^8.53.0",
    "@typescript-eslint/parser": "^8.53.0",
    "autoprefixer": "^10.4.23",
    "commander": "^14.0.2",
    "depcheck": "^1.4.7",
    "esbuild": "0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "16.1.3",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "firebase-tools": "^15.3.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.0",
    "rimraf": "^6.1.2",
    "serve": "^14.2.5",
    "shadcn": "^3.7.0",
    "tailwindcss": "^4.1.18",
    "ts-jest": "^29.4.6",
    "ts-node": "^1.7.1",
    "tsx": "^4.21.0",
    "typescript": "5.9.3",
    "which": "6.0.0"
  },
  "overrides": {
    "esbuild": "0.27.2",
    "test-exclude": "7.0.1",
    "@react-three/drei": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@react-three/postprocessing": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
