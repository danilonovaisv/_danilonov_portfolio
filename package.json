{
  "name": "danilo-novais-portfolio",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "keywords": [
    "react",
    "three",
    "threejs",
    "react-three-fiber"
  ],
  "engines": {
    "node": ">=22"
  },
  "packageManager": "pnpm@10.28.0",
  "scripts": {
    "dev": "next dev",
    "cleandev": "rm -rf .next && pnpm dev",
    "prebuild": "node scripts/generate-build-info.cjs",
    "build": "next build",
    "start": "serve out",
    "clean": "rimraf .next out",
    "update": "npx npm-check-updates -u && pnpm install",
    "lint": "eslint --cache --cache-location .eslintcache .",
    "lint:fix": "eslint --cache --cache-location .eslintcache . --fix",
    "eslint:ci": "eslint .",
    "prettier": "prettier --check .",
    "prettier-fix": "prettier --write .",
    "typecheck": "tsc --noEmit --strict --jsx react-jsx",
    "typegen": "tsc --emitDeclarationOnly",
    "analyze": "pnpm run lint && pnpm run typecheck",
    "analyze:deps": "depcheck",
    "analyze:graph": "node scripts/analyze-graph.cjs",
    "audit:project": "npx ts-node scripts/analyze-project.ts",
    "test": "pnpm run eslint:ci && pnpm run typecheck && pnpm run prettier && pnpm run jest",
    "jest": "jest",
    "format": "prettier --write .",
    "deploy": "pnpm run build && firebase deploy --only hosting --project portfolio-danilo-novais"
  },
