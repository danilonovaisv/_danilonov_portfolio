{
  "name": "danilo-novais-portfolio",
  "private": true,
  "version": "1.0.1",
  "type": "module",
  "keywords": [
    "react",
    "three",
    "threejs",
    "react-three-fiber"
  ],
  "engines": {
    "node": "20"
  },
  "scripts": {
    "dev": "next dev --webpack",
    "start": "next start --hostname 0.0.0.0 --port ${PORT:-3000}",
    "clean": "rimraf .next out .eslintcache .firebase dist deploy-public",
    "prebuild": "node scripts/generate-build-info.cjs",
    "validate-env": "tsx scripts/validate-env.ts",
    "build": "pnpm run validate-env && pnpm run prebuild && next build --webpack",
    "prepare-deploy": "tsx scripts/prep-deploy.ts",
    "predeploy": "tsx scripts/validate-deploy-config.ts && python3 scripts/audit_assets.py",
    "deploy": "pnpm run predeploy && bash scripts/deploy.sh",
    "deploy:hosting": "pnpm run build && bash scripts/prepare-hosting.sh && firebase deploy --only hosting --debug",
    "test-and-deploy": "tsx scripts/run-full-test-and-deploy.ts",
    "admin:deploy": "pnpm run admin:prepare && pnpm run build",
    "admin:prepare": "pnpm run validate-env && tsx scripts/repair-site-assets-paths.ts && tsx scripts/update-supabase-urls.ts && tsx scripts/check-storage-links.ts && tsx scripts/verify-asset-corrections.ts",
    "postinstall": "if [ -d scripts ]; then pnpm run prebuild; else echo 'Skipping prebuild: scripts directory not found'; fi",
    "lint": "eslint src test tailwind.config.ts",
    "lint:fix": "eslint src test tailwind.config.ts --fix && prettier --write .",
    "format": "prettier --write .",
    "typecheck": "tsc --noEmit --strict --jsx react-jsx",
    "test": "pnpm run lint && pnpm run typecheck && jest",
    "test:watch": "jest --watch",
    "test:e2e": "playwright test",
    "analyze": "pnpm run lint && pnpm run typecheck",
    "analyze:deps": "depcheck",
    "analyze:bundle": "ANALYZE=true next build --webpack",
    "assets:sync": "tsx scripts/sync-site-assets.ts assets.json",
    "assets:audit": "python3 scripts/audit_assets.py",
    "manage-keys": "tsx scripts/manage-service-account-keys.ts",
    "update": "pnpm dlx npm-check-updates -u && pnpm install",
    "tools": "bash scripts/project-tools.sh",
    "clawdbot:analyze": "clawdbot chat --profile pnpm-next-portfolio",
    "clawdbot:perf": "clawdbot exec --script webgl-performance-audit",
    "clawdbot:seo": "clawdbot exec --script lighthouse-audit"
  },
  "dependencies": {
    "@dataconnect/admin-generated": "file:src/dataconnect-admin-generated",
    "@dataconnect/generated": "file:src/dataconnect-generated",
    "@genkit-ai/google-genai": "^1.28.0",
    "@google/adk": "^0.3.0",
    "@gsap/react": "^2.1.2",
    "@hookform/resolvers": "^5.2.2",
    "@radix-ui/react-accordion": "^1.2.12",
    "@radix-ui/react-alert-dialog": "^1.1.15",
    "@radix-ui/react-avatar": "^1.1.11",
    "@radix-ui/react-checkbox": "^1.3.3",
    "@radix-ui/react-collapsible": "^1.1.12",
    "@radix-ui/react-dialog": "^1.1.15",
    "@radix-ui/react-dropdown-menu": "^2.1.16",
    "@radix-ui/react-label": "^2.1.8",
    "@radix-ui/react-menubar": "^1.1.16",
    "@radix-ui/react-popover": "^1.1.15",
    "@radix-ui/react-progress": "^1.1.8",
    "@radix-ui/react-radio-group": "^1.3.8",
    "@radix-ui/react-scroll-area": "^1.2.10",
    "@radix-ui/react-select": "^2.2.6",
    "@radix-ui/react-separator": "^1.1.8",
    "@radix-ui/react-slider": "^1.3.6",
    "@radix-ui/react-slot": "^1.2.4",
    "@radix-ui/react-switch": "^1.2.6",
    "@radix-ui/react-tabs": "^1.1.13",
    "@radix-ui/react-toast": "^1.2.15",
    "@radix-ui/react-tooltip": "^1.2.8",
    "@react-three/drei": "^10.7.7",
    "@react-three/fiber": "^9.5.0",
    "@react-three/postprocessing": "^3.0.4",
    "@supabase/ssr": "^0.8.0",
    "@supabase/supabase-js": "^2.94.1",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "embla-carousel-react": "^8.6.0",
    "firebase": "^12.8.0",
    "firebase-admin": "^13.6.1",
    "firebase-frameworks": "^0.11.8",
    "firebase-functions": "^7.0.5",
    "framer-motion": "^12.31.0",
    "gsap": "^3.14.2",
    "husky": "^9.1.7",
    "lenis": "^1.3.17",
    "lightningcss": "1.31.1",
    "lint-staged": "^16.2.7",
    "lucide-react": "^0.563.0",
    "maath": "^0.10.8",
    "motion": "^12.31.0",
    "motion-studio-mcp": "https://api.motion.dev/registry.tgz?package=motion-studio-mcp&version=latest",
    "next": "^16.1.6",
    "openai": "^6.17.0",
    "postprocessing": "^6.38.2",
    "react": "^19.2.4",
    "react-day-picker": "^9.13.0",
    "react-dom": "^19.2.4",
    "react-hook-form": "^7.71.1",
    "react-markdown": "^10.1.0",
    "recharts": "^3.7.0",
    "statsig-js": "^5.1.0",
    "tailwind-merge": "^3.4.0",
    "three": "^0.182.0",
    "three-stdlib": "^2.36.1",
    "uuid": "^13.0.0",
    "zod": "^4.3.6",
    "zustand": "^5.0.11"
  },
  "devDependencies": {
    "@jest/globals": "^30.2.0",
    "@next/bundle-analyzer": "^16.1.6",
    "@next/eslint-plugin-next": "^16.1.6",
    "@playwright/test": "^1.58.1",
    "@tailwindcss/postcss": "^4.1.18",
    "@tailwindcss/typography": "^0.5.19",
    "@testing-library/dom": "^10.4.1",
    "@testing-library/jest-dom": "^6.9.1",
    "@testing-library/react": "^16.3.2",
    "@testing-library/user-event": "^14.6.1",
    "@types/handlebars": "^4.1.0",
    "@types/jest": "^30.0.0",
    "@types/node": "25.2.0",
    "@types/react": "19.2.11",
    "@types/react-dom": "19.2.3",
    "@types/three": "^0.182.0",
    "@typescript-eslint/eslint-plugin": "^8.54.0",
    "@typescript-eslint/parser": "^8.54.0",
    "autoprefixer": "^10.4.24",
    "commander": "^14.0.3",
    "depcheck": "^1.4.7",
    "esbuild": "0.27.2",
    "eslint": "^9.39.2",
    "eslint-config-next": "^16.1.6",
    "eslint-config-prettier": "^10.1.8",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.5.5",
    "eslint-plugin-react": "^7.37.5",
    "firebase-tools": "^15.5.1",
    "jest": "^30.2.0",
    "jest-environment-jsdom": "^30.2.0",
    "knip": "^5.83.0",
    "postcss": "^8.5.6",
    "prettier": "^3.8.1",
    "rimraf": "^6.1.2",
    "serve": "^14.2.5",
    "shadcn": "^3.8.3",
    "styled-jsx": "^5.1.7",
    "tailwindcss": "4.1.18",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "tsx": "^4.21.0",
    "tweakpane": "^4.0.5",
    "typescript": "5.9.3",
    "which": "6.0.0"
  },
  "overrides": {
    "esbuild": "0.27.2",
    "test-exclude": "7.0.1",
    "tar": "7.5.7",
    "hono": "4.11.7",
    "fast-xml-parser": "5.3.4",
    "cookie": "0.7.0",
    "@react-three/drei": {
      "react": "$react",
      "react-dom": "$react-dom"
    },
    "@react-three/postprocessing": {
      "react": "$react",
      "react-dom": "$react-dom"
    }
  }
}
